{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style":"off",
        "jsx-filename-extension":"off",
        "react/prefer-stateless-function": "off",
        "react/jsx-filename-extension":"off",
        "react/jsx-one-expression-per-line":"off",
        "no-use-before-define":"off"
    }
}